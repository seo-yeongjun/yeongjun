<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="news.newsCompany">
    <select id="selectAllCompany" resultType="com.yeongjun.yeongjun.news.entity.NewsCompany">
        SELECT
               company_name, part_of_link
        FROM news_company
    </select>
</mapper>
